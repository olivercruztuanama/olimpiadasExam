@{
    ViewBag.Title = "Prestamos";
}
<script type="text/javascript">
        var siteRoot = "@Url.Content("~/")";
</script>
<input type="hidden" id="page_" value="@Model.Pageview" />
<input type="hidden" id="id" value="0" />
<input type="hidden" id="hora" value="0" />
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Listado de Equipamientos</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("Dashboard_1", "Dashboards")">Inicio</a>
            </li>
            <li class="active">
                <strong>Equipamientos</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">
    </div>
</div>

<a class="dropdown-toggle" data-toggle="modal" data-target="#formulario" href="#" id="popupFrm"></a>
<div class="modal inmodal fade" id="formulario" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content animated fadeInRight">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h3 class="modal-title">Equipamiento</h3>
            </div>
            <div class="modal-body">
                <form role="form">
                    <div class="form-group row">
                        <label class="col-lg-2 col-form-label">Equipamiento</label>
                        <div class="col-lg-6"><input type="text" id="fr_equipamiento" class="form-control" placeholder="Ingrese un equipamiento" autocomplete="off" /></div>
                    </div>
                    <div class="form-group row">
                        <label class="col-lg-2 col-form-label">Estado</label>
                        <div class="col-lg-6">
                            <select class="chosen-select form-control" id="fr_estado"></select>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-white" data-dismiss="modal" id="idClose">Cerrar</button>
                <button type="button" class="btn btn-primary" id="idSave">Guardar</button>
            </div>
        </div>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <div class="ibox-tools">
                        <a class="dropdown-toggle" title="Agregar" onclick="add_()">
                            <img src="~/Contenido/Images/Plus.png" />
                        </a>
                    </div>
                </div>
                <div class='table-responsive' style="background-color:#ffffff">
                    <div class="ibox-content" id="lista1"></div>
                </div>
            </div>
        </div>
    </div>
</div>

<input type="hidden" id="fr_id" />
@section Styles {
    @Styles.Render("~/Contenido/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/Contenido/plugins/dropzone/dropZoneStyles")
    @Styles.Render("~/plugins/jasnyBootstrapStyles")
    @Styles.Render("~/plugins/select2Styles")
    @Styles.Render("~/Contenido/plugins/chosen/chosenStyles")
    @Styles.Render("~/plugins/dataPickerStyles")
    @Styles.Render("~/Contenido/plugins/iCheck/iCheckStyles")
    @Styles.Render("~/Contenido/plugins/blueimp/css/")
    @Styles.Render("~/Contenido/plugins/signature/signatureStyles")
    <link href="~/Contenido/bootstrap.css" rel="stylesheet" />
}
@section Scripts {
    @Scripts.Render("~/plugins/dataTables")
    @Scripts.Render("~/plugins/jasnyBootstrap")
    @Scripts.Render("~/plugins/select2")
    @Scripts.Render("~/plugins/chosen")
    @Scripts.Render("~/plugins/dataPicker")
    @Scripts.Render("~/plugins/iCheck")
    @Scripts.Render("~/plugins/typehead")
    @Scripts.Render("~/plugins/dropZone")
    @Scripts.Render("~/plugins/lightboxGallery")
    @Scripts.Render("~/plugins/signature")
    <script src="~/Scripts/plugins/chosen/chosen.jquery.js?v=1.0.1"></script>
    <script src="~/Scripts/plugins/dataTables/datatables.min.js?v=1.1"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/momentjs/latest/moment.min.js"></script>
    <script src="~/Contenido/Scripts/equipamientos.js?v=1.0.0"></script>
}